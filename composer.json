{
    "autoload": {
        "psr-4": {
            "ArkonExample\\": "src/"
        }
    },
    "config": {
        "autoloader-suffix": "ArkonExample",
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "require": {
        "php": "^8.1",
        "arkonsoft/ps-module-core": "^1.1",
        "arkonsoft/ps-module-di": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse --memory-limit 1G"
    },
    "type": "prestashop-module"
}
